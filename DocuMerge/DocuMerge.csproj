<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="images\buttons\ADD_PUSH.svg" />
	  <None Remove="images\buttons\ADD_Sign.svg" />
	  <None Remove="images\buttons\CLOSE_PUSH.svg" />
	  <None Remove="images\buttons\CLOSE_Sign.svg" />
	  <None Remove="images\buttons\COMBIND_PUSH.svg" />
	  <None Remove="images\buttons\COMBIND_Sign.svg" />
