	  <Content Include="images\buttons\ADD_Sign.svg">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="images\buttons\CLOSE_PUSH.svg">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	  <Content Include="images\buttons\CLOSE_Sign.svg">
