	  <None Remove="images\buttons\PREVIEW_Sign.svg" />
	  <None Remove="images\buttons\PRINT_PUSH.svg" />
	  <None Remove="images\buttons\PRINT_Sign.svg" />
	  <None Remove="images\DOCUMERGE_banner.png" />
	</ItemGroup>
	<ItemGroup>
	  <Content Include="images\buttons\ADD_PUSH.svg">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
